<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle2</base-name>
            <var>bundle2</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>RiesgoConverter</managed-bean-name>
        <managed-bean-class>controlador.converters.RiesgoConverter</managed-bean-class>
        
        <managed-bean-scope>application</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>TipoDeAplicacioConverter</managed-bean-name>
        <managed-bean-class>controlador.converters.TipoDeAplicacionConverter</managed-bean-class>
        
        <managed-bean-scope>application</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>TipoDeFresaConverter</managed-bean-name>
        <managed-bean-class>controlador.converters.TipoDeFresaConverter</managed-bean-class>
        
        <managed-bean-scope>application</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>TipoDeValoracionConverter</managed-bean-name>
        <managed-bean-class>controlador.converters.TipoDeValoracionConverter</managed-bean-class>
        
        <managed-bean-scope>application</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>UnidadesConverter</managed-bean-name>
        <managed-bean-class>controlador.converters.UnidadesConverter</managed-bean-class>
        
        <managed-bean-scope>application</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>RolConverter</managed-bean-name>
        <managed-bean-class>controlador.converters.RolConverter</managed-bean-class>
        
        <managed-bean-scope>application</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>SiNoConverter</managed-bean-name>
        <managed-bean-class>controlador.converters.BooleanConverter</managed-bean-class>
        
        <managed-bean-scope>application</managed-bean-scope></managed-bean>
</faces-config>
